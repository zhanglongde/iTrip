@{
    ViewBag.Title = "同游网-找回密码";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="../../Content/register/CSS/findpassword.css" rel="stylesheet" type="text/css" />
<link href="../../Content/poshytip/tip-yellow/tip-yellow.css" rel="stylesheet" type="text/css" />
<link href="../../Content/poshytip/tip-yellowsimple/tip-yellowsimple.css" rel="stylesheet"
    type="text/css" />
<div class="FindPwd_Main">
    <div class="FindPwd_head">
        <span>找回密码</span>
    </div>
    <table>
        <tbody class="FP_tbody">
            <tr>
                <th>
                    邮箱地址:
                </th>
                <td>
                    <input type="text" id="emailAdr" />
                </td>
            </tr>
            <tr>
                <th>
                    用户名:
                </th>
                <td>
                    <input type="text" id="userName" />
                </td>
            </tr>
            <tr>
                <th>
                    设置新密码:
                </th>
                <td>
                    <input type="password" id="pwd" />
                </td>
            </tr>
            <tr>
                <th>
                    确认新密码:
                </th>
                <td>
                    <input type="password" id="confirmpwd" />
                </td>
            </tr>
            <tr>
                <th>
                </th>
                <td>
                    <a class="confirmSend" id="confirmSend"></a>
                </td>
            </tr>
        </tbody>
    </table>
    <span class="emailPrompt">邮件已发送请及时查收!</span>
</div>
<script type="text/javascript">
    var _bdhmProtocol = (("https:" == document.location.protocol) ? " https://" : " http://");
    document.write(unescape("%3Cscript src='" + _bdhmProtocol + "hm.baidu.com/h.js%3F661e06f6cdce83549f28e3b5a0aedab5' type='text/javascript'%3E%3C/script%3E"));
</script>
<script src="../../Scripts/jquery-1.4.4.js" type="text/javascript"></script>
<script src="../../Content/poshytip/jquery.poshytip.js" type="text/javascript"></script>
<script src="../../Content/register/javacript/findpassword.js" type="text/javascript"></script>
